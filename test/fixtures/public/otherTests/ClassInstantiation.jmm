import static ioa.println(int);
import Pair;
import Pair(int);
import Pair(int, int);
import Pair.getX() int;
import Pair.getY() int;
import Pair.printPair();

class ClassInstantiation {
    int x;

	public static void main(String[] args) {
        int a;
        ClassInstantiation c1;
        ClassInstantiation c2;
        ClassInstantiation c3;
        Pair pair1;
        Pair pair2;
        Pair pair3;

        a = 1;
        c1 = new ClassInstantiation();
        c1.setX(a);

        c2 = new ClassInstantiation();
        c2.setX(2);

        c3 = new ClassInstantiation();
        c3.setX(c3.add(1, 2));

        ioa.println(c1.getX());
        ioa.println(c2.getX());
        ioa.println(c3.getX());

        pair1 = new Pair();
        pair2 = new Pair(1);
        pair3 = new Pair(10, 2);

        ioa.println(pair1.getX());
        ioa.println(pair1.getY());
        pair1.printPair();

        ioa.println(pair2.getX());
        ioa.println(pair2.getY());
        pair2.printPair();

        ioa.println(pair3.getX());
        ioa.println(pair3.getY());
        pair3.printPair();
    }

    public int setX(int newX){
        x = newX;
        return x;
    }

    public int getX(){
        return x;
    }

	public int add(int a, int b){
		return a+b;
	}
}

