import static io.println(int);

/**
 * This test serves to test the compiler's capability to generate
 * block statements (that is, if/else and while statements) using
 * complex expressions and nested blocks
 */
class BlockStatementTest {

    public static void main(String[] args) {
        int x;
        int y;
        int i;
        i = 0;
        x = 10;

        while(i< 10){

            if ((i * 2)  < 6) {

                if (!(i + 2 < 4)) {
                    x = 1;
                    if (true) {
                        x = 2;
                    }
                    else {
                    }
                }
                else {

                }
            }
            else {
                while(!(x < 3)){
                    x = x- 1;
                }
            }
            i = i+1;
        }

        y = x+1;
        io.println(y);
    }

}