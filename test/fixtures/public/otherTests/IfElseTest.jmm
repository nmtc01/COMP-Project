/**
	Test for if..else statements
**/

import static ioPlus.printHelloWorld(void) void;
import static io.println(int);

class IfElseTest {

	public static void main(String[] args) {
		int x;
		int y;
		int z;

		ioPlus.printHelloWorld();

		if (true) {
			if (true) {
				x = 1;
				if (true) {
					x = 2;
				}
				else {
					x = 3;
				}
			}
			else {
				if (true) {
					x = 4;
				}
				else {
					x = 5;
				}
			}
		}
		else {
			if (true) {
				x = 6;
			}
			else {
				x = 7;
			}
		}

		y = x+1;
		io.println(y);
	}

}